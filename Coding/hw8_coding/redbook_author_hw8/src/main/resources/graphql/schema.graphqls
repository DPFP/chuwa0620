type Post{
    id:ID
    title:String
    description:String
    content:String
}

type Query{
    postById(id:ID): Post
    authorById(id:ID): Author
}

type Mutation{
    createPost(title:String!, description:String!, content:String!) : Post!
    createAuthor(id:String!, birthday:String!, name:String!) : Author!
}

type Author{
    id:ID
    birthday:String
    name:String
}