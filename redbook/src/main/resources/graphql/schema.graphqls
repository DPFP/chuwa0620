type Post{
    id:ID
    title:String
    description:String
    content:String
}

type Query{
    postById(id:ID): Post
}

type Mutation{
    createPost(title:String!, description:String!, content:String!) : Post!
}

type Author{
    id:ID
    username:String
    age:Int
}

type Query{
    postById(id:ID): Author
}

type Mutation{
    createPost( username:String!, age:Int!) : Author!
}